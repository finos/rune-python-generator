namespace rosetta_dsl.test.semantic.expressions.type_conversion : <"generate Python unit tests from Rosetta.">

func StringToInt:
    inputs:
        s string(1..1)
    output:
        res int(1..1)
    set res:
        s to-int

func IntToString:
    inputs:
        i int(1..1)
    output:
        res string(1..1)
    set res:
        i to-string
